    "import requests\n",
    "import lxml.html\n",
    "\n",
    "r = requests.get('https://www.xoom.com/philippines/send-money')\n",
    "data = r.text\n",
    "\n",
    "tree = lxml.html.fromstring(data)\n",
    "\n",
    "rate = tree.xpath(\"//div[@class='js-exchange-rate']\")\n",
    "\n",
    "rate[0].text_content()"
